<odoo>
    <data>
        <record model="ir.cron" forcecreate="True" id="runescape.cron_respawn">
            <field name="name">Game: Cron Respawn</field>
            <field name="model_id" ref="model_runescape_dungeon_mob_rel"/>
            <field name="state">code</field>
            <field name="code">model.respawn_mobs()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="activity_user_type">specific</field>
            <field name="doall" eval="False" />
        </record>
        <record model="ir.cron" forcecreate="True" id="runescape.cron_battles">
            <field name="name">Game: Update Battles</field>
            <field name="model_id" ref="model_runescape_battle"/>
            <field name="state">code</field>
            <field name="code">model.update_battle()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="activity_user_type">specific</field>
            <field name="doall" eval="False" />
        </record>
    </data>
</odoo>