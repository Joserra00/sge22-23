<odoo>
<data>
<record model="ir.ui.view" id="runescape.player_list">
      <field name="name">runescape list</field>
      <field name="model">runescape.player</field>
      <field name="arch" type="xml">
        <tree>
            <field name="avatar" widget="image"/>
          <field name="name"/>
          <field name="coins"/>
          <field name="strength"/>
        </tree>
      </field>
    </record>
     <record model="ir.ui.view" id="runescape.player_form">
      <field name="name">runescape list</field>
      <field name="model">runescape.player</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
                <group><field name="avatar" widget="image"/>
                <field name="name"/>
                <field name="password" password = "true"/>
                <field name="hp"/>
                    <field name="coins"/>
                <field name="strength"/>
                    </group>
                <group>
                    <field name="armor_image" widget="image"/>
                    <field name="armor" mode="tree"/>
                    <field name="defense"/>
                    <field name="sword_image" widget="image"/>
                    <field name="sword" mode="tree"/>
                    <field name="damage"/>
                </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    </data>
</odoo>