    <!-- Top menu item -->

    <menuitem name="runescape" id="runescape.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu" id="runescape.menu_game" parent="runescape.menu_root"/>
    <menuitem name="Battle" id="runescape.battle_game" parent="runescape.menu_root"/>

    <!-- actions -->

    <menuitem name="Player list" id="runescape.menu_player_list" parent="runescape.menu_game"
              action="runescape.action_player_window"/>
    <menuitem name="Armor " id="runescape.menu_armor_list" parent="runescape.menu_game"
              action="runescape.action_armor_window"/>
    <menuitem name="Sword " id="runescape.menu_sword_list" parent="runescape.menu_game"
              action="runescape.action_sword_window"/>
    <menuitem name="Safe Zone" id="runescape.menu_zone_list" parent="runescape.menu_game"
              action="runescape.action_zone_window"/>
    <menuitem name="Dungeon" id="runescape.menu_dungeon_list" parent="runescape.menu_game"
              action="runescape.action_dungeon_window"/>
    <menuitem name="Mob" id="runescape.menu_mob_list" parent="runescape.menu_game"
              action="runescape.action_mob_window"/>
    <menuitem name="Travel" id="runescape.menu_travel_dungeon_list" parent="runescape.battle_game"
              action="runescape.action_travel_dungeon_window"/>



  </data>
</odoo>