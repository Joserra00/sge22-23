<odoo>
  <data>
    <!-- explicit list view definition -->
     


       <record model="ir.ui.view" id="runescape.dungeon_mob_rel_list">
      <field name="name">runescape </field>
      <field name="model">runescape.dungeon_mob_rel</field>
      <field name="arch" type="xml">
        <tree>
            <field name="name" />
            <field name="dungeon"/>
          <field name="mob"/>
          <field name="qty"/>
        </tree>
      </field>
    </record>
      
    




    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="runescape.action_player_window">
      <field name="name">runescape</field>
      <field name="res_model">res.partner</field>
      <field name="view_mode">kanban,tree,form</field>
      <!--<field name="domain"></field>
      <field name="search_view_id" ref="runescape.player_search"></field>
      <field name="context">{'search_default_player_partner': 1, 'default_is_player': True, }</field>
      <field name="view_ids" eval="[(5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('runescape.player_list')}),
            (0, 0, {'view_mode': 'form', 'view_id': ref('runescape.player_form')}),]" />-->
    </record>
        <record model="ir.actions.act_window" id="runescape.action_armor_window">
      <field name="name">Armor</field>
      <field name="res_model">runescape.armor</field>
      <field name="view_mode">tree,form</field>
    </record>
        <record model="ir.actions.act_window" id="runescape.action_sword_window">
      <field name="name">Sword</field>
      <field name="res_model">runescape.sword</field>
      <field name="view_mode">tree,form</field>
    </record>
   <record model="ir.actions.act_window" id="runescape.action_zone_window">
      <field name="name">Safe Zone</field>
      <field name="res_model">runescape.zone</field>
      <field name="view_mode">kanban,tree,form</field>
    </record>
       <record model="ir.actions.act_window" id="runescape.action_dungeon_window">
      <field name="name">Dungeon</field>
      <field name="res_model">runescape.dungeon</field>
      <field name="view_mode">kanban,tree,form</field>
    </record>
       <record model="ir.actions.act_window" id="runescape.action_mob_window">
      <field name="name">Mob</field>
      <field name="res_model">runescape.mob</field>
      <field name="view_mode">tree,form</field>
    </record>
      <record model="ir.actions.act_window" id="runescape.action_travel_dungeon_window">
      <field name="name">Travel</field>
      <field name="res_model">runescape.travel_dungeon</field>
      <field name="view_mode">tree,form,calendar</field>
    </record>
        <record model="ir.actions.act_window" id="runescape.action_battle_window">
      <field name="name">Battle</field>
      <field name="res_model">runescape.battle</field>
      <field name="view_mode">tree,form</field>
      <field name="context">{'search_default_battle_partner': 1, 'default_battle_ended': False}</field>
    </record>


    <!-- server action to the one above -->
<!--
    <record model="ir.actions.server" id="runescape.action_server">
      <field name="name">runescape server</field>
      <field name="model_id" ref="model_runescape_runescape"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>
-->

    <!-- Top menu item -->

    <menuitem name="runescape" id="runescape.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu" id="runescape.menu_game" parent="runescape.menu_root"/>
    <menuitem name="Travel/Battle" id="runescape.battle_game" parent="runescape.menu_root"/>

    <!-- actions -->

    <menuitem name="Player list" id="runescape.menu_player_list" parent="runescape.menu_game"
              action="runescape.action_player_window"/>
    <menuitem name="Armor " id="runescape.menu_armor_list" parent="runescape.menu_game"
              action="runescape.action_armor_window"/>
    <menuitem name="Sword " id="runescape.menu_sword_list" parent="runescape.menu_game"
              action="runescape.action_sword_window"/>
    <menuitem name="Safe Zone" id="runescape.menu_zone_list" parent="runescape.menu_game"
              action="runescape.action_zone_window"/>
    <menuitem name="Dungeon" id="runescape.menu_dungeon_list" parent="runescape.menu_game"
              action="runescape.action_dungeon_window"/>
    <menuitem name="Mob" id="runescape.menu_mob_list" parent="runescape.menu_game"
              action="runescape.action_mob_window"/>
    <menuitem name="Travel" id="runescape.menu_travel_dungeon_list" parent="runescape.battle_game"
              action="runescape.action_travel_dungeon_window"/>
    <menuitem name="Battle" id="runescape.menu_battle_list" parent="runescape.battle_game"
              action="runescape.action_battle_window"/>



  </data>
</odoo>
