<odoo>
  <data>
    <!-- explicit list view definition -->

      <record model="ir.ui.view" id="runescape.sword_form">
      <field name="name">runescape sword</field>
      <field name="model">runescape.sword</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
                <field name="sword_image" widget="image"/>
                <field name="name"/>
                <field name="damage"/>
                <field name="price"/>
                <field name="material"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
      <record model="ir.ui.view" id="runescape.sword_list">
      <field name="name">runescape swords</field>
      <field name="model">runescape.sword</field>
      <field name="arch" type="xml">
        <tree>
            <field name="sword_image" widget="image"/>
            <field name="name"/>
          <field name="damage"/>
          <field name="price"/>
        </tree>
      </field>
    </record>
      <record model="ir.ui.view" id="runescape.armor_form">
      <field name="name">runescape armor</field>
      <field name="model">runescape.armor</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="armor_image" widget="image"/>
                <field name="name"/>
                <field name="defense"/>
                <field name="price"/>
                <field name="material"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
      <record model="ir.ui.view" id="runescape.armor_list">
      <field name="name">runescape armor</field>
      <field name="model">runescape.armor</field>
      <field name="arch" type="xml">
        <tree>
            <field name="armor_image" widget="image"/>
          <field name="defense"/>
          <field name="price"/>
          <field name="name"/>
        </tree>
      </field>
    </record>



       <record model="ir.ui.view" id="runescape.dungeon_mob_rel_list">
      <field name="name">runescape </field>
      <field name="model">runescape.dungeon_mob_rel</field>
      <field name="arch" type="xml">
        <tree>
            <field name="name" />
            <field name="dungeon"/>
          <field name="mob"/>
          <field name="qty"/>
        </tree>
      </field>
    </record>
      <record model="ir.ui.view" id="runescape.dungeon_travel_dungeon_rel_list">
      <field name="name">runescape </field>
      <field name="model">runescape.travel_dungeon</field>
      <field name="arch" type="xml">
        <tree>
            <field name="player" />
            <field name="travel_to"/>
            <field name="dungeon"/>
            <field name="zone" />


        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="runescape.dungeon_travel_dungeon_rel_form">
      <field name="name">runescape </field>
      <field name="model">runescape.travel_dungeon</field>
      <field name="arch" type="xml">
          <form>
             <header>
              
            </header>
              <sheet>
        <group>
            <field name="player" />
            <field name="travel_to"/>
            <field name="dungeon" attrs="{'invisible':[('travel_to', '=', '1')]}"/>
            <field name="zone" attrs="{'invisible':[('travel_to', '=', '2')]}"/>
            <field name="date_start"/>
            <field name="date_end"/>
            <button name="battle_mob" type="object" string="Battle" class="oe_highlight" attrs="{'invisible':[('travel_to', '!=', '2')]}" />
            <field name="mob" attrs="{'invisible':[('travel_to', '=', '1')]}"/>
           

        </group>
              </sheet>
          </form>
      </field>
    </record>
    




    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="runescape.action_player_window">
      <field name="name">runescape</field>
      <field name="res_model">runescape.player</field>
      <field name="view_mode">kanban,tree,form</field>
    </record>
        <record model="ir.actions.act_window" id="runescape.action_armor_window">
      <field name="name">Armor</field>
      <field name="res_model">runescape.armor</field>
      <field name="view_mode">tree,form</field>
    </record>
        <record model="ir.actions.act_window" id="runescape.action_sword_window">
      <field name="name">Sword</field>
      <field name="res_model">runescape.sword</field>
      <field name="view_mode">tree,form</field>
    </record>
   <record model="ir.actions.act_window" id="runescape.action_zone_window">
      <field name="name">Safe Zone</field>
      <field name="res_model">runescape.zone</field>
      <field name="view_mode">kanban,tree,form</field>
    </record>
       <record model="ir.actions.act_window" id="runescape.action_dungeon_window">
      <field name="name">Dungeon</field>
      <field name="res_model">runescape.dungeon</field>
      <field name="view_mode">kanban,tree,form</field>
    </record>
       <record model="ir.actions.act_window" id="runescape.action_mob_window">
      <field name="name">Mob</field>
      <field name="res_model">runescape.mob</field>
      <field name="view_mode">tree,form</field>
    </record>
      <record model="ir.actions.act_window" id="runescape.action_travel_dungeon_window">
      <field name="name">Travel</field>
      <field name="res_model">runescape.travel_dungeon</field>
      <field name="view_mode">tree,form,calendar</field>
    </record>


    <!-- server action to the one above -->
